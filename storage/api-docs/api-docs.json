    "paths": {
        "/auth/forgot-password": {
            "post": {
                "tags": [
                    "Reset Password"
                ],
                "summary": "Send Password Reset Email",
                "description": "Send password reset link to user's email",
                "operationId": "83741a4d451a01846f9ccd53bce3590f",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "description": "User's email or phone number",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "example": {
                                "email": "user@example.com"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Success message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Error message describing the validation issue",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/auth/validate-otp": {
            "post": {
                "tags": [
                    "Reset Password"
                ],
                "summary": "Validate OTP",
                "description": "Validate the OTP sent to user's email",
                "operationId": "60b50aecb340b98f280c115af6183a65",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "description": "User's email or phone number",
                                        "type": "string"
                                    },
                                    "otp": {
                                        "description": "OTP sent to user's email or phone",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "example": {
                                "email": "user@example.com",
                                "otp": "123456"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "description": "User ID",
                                                    "type": "integer"
                                                },
                                                "name": {
                                                    "description": "User name",
                                                    "type": "string"
                                                },
                                                "email": {
                                                    "description": "User email",
                                                    "type": "string"
                                                },
                                                "phone": {
                                                    "description": "User phone number",
                                                    "type": "string"
                                                },
                                                "otp": {
                                                    "description": "OTP for password reset",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Error message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/auth/set-password": {
            "post": {
                "tags": [
                    "Reset Password"
                ],
                "summary": "Set user password after OTP validation",
                "description": "Set user password after OTP validation",
                "operationId": "dec64df20ff270322d212daa89459c21",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "description": "User's email or phone number",
                                        "type": "string"
                                    },
                                    "otp": {
                                        "description": "OTP sent to user's email or phone",
                                        "type": "string"
                                    },
                                    "password": {
                                        "description": "New password for the user",
                                        "type": "string"
                                    },
                                    "password_confirmation": {
                                        "description": "Password confirmation",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "example": {
                                "email": "user@example.com",
                                "otp": "123456",
                                "password": "NewPassword123",
                                "password_confirmation": "NewPassword123"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "description": "User ID",
                                                    "type": "integer"
                                                },
                                                "name": {
                                                    "description": "User name",
                                                    "type": "string"
                                                },
                                                "email": {
                                                    "description": "User email",
                                                    "type": "string"
                                                },
                                                "phone": {
                                                    "description": "User phone number",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Error message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Authentication V1"
                ],
                "summary": "User Login",
                "description": "Authenticate user and return access token",
                "operationId": "18294b29b61e586ad50e52197084f8e9",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "description": "User's email or phone number",
                                        "type": "string"
                                    },
                                    "password": {
                                        "description": "User's password",
                                        "type": "string"
                                    },
                                    "device_type": {
                                        "description": "Type of device used for login",
                                        "type": "string",
                                        "enum": [
                                            "ios",
                                            "android"
                                        ]
                                    },
                                    "device_token": {
                                        "description": "Device token for push notifications",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "example": {
                                "email": "user@example.com",
                                "password": "password123",
                                "device_type": "ios",
                                "device_token": "device_token_example"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "userinfo": {
                                            "description": "Authenticated user details",
                                            "type": "object"
                                        },
                                        "token": {
                                            "description": "Access token for authenticated user",
                                            "type": "string"
                                        },
                                        "token_type": {
                                            "description": "Type of the token (e.g., Bearer)",
                                            "type": "string"
                                        },
                                        "expires_at": {
                                            "description": "Token expiration time in ISO 8601 format",
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Error message describing the validation issue",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/auth/logout": {
            "post": {
                "tags": [
                    "Authentication V1"
                ],
                "summary": "User Logout",
                "description": "Logout user and revoke access token",
                "operationId": "4b88b902a0ef23e6d971c5440c911cbf",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Logout success message",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/dashboard": {
            "get": {
                "tags": [
                    "Dashboard"
                ],
                "summary": "Get Dashboard Data",
                "description": "Get authenticated user details and dashboard information",
                "operationId": "9bcd6451d893e421d0b57103c4296268",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "user": {
                                                    "description": "Authenticated user details",
                                                    "type": "object"
                                                },
                                                "dashboard": {
                                                    "description": "Dashboard information",
                                                    "properties": {
                                                        "stats": {
                                                            "description": "Dashboard statistics",
                                                            "properties": {
                                                                "total_users": {
                                                                    "description": "Total number of users",
                                                                    "type": "integer"
                                                                },
                                                                "active_users": {
                                                                    "description": "Number of active users",
                                                                    "type": "integer"
                                                                },
                                                                "new_signups": {
                                                                    "description": "Number of new signups",
                                                                    "type": "integer"
                                                                }
                                                            },
                                                            "type": "object"
                                                        },
                                                        "recent_activity": {
                                                            "description": "List of recent activities",
                                                            "type": "array",
                                                            "items": {
                                                                "properties": {
                                                                    "type": {
                                                                        "description": "Type of activity",
                                                                        "type": "string"
                                                                    },
                                                                    "timestamp": {
                                                                        "description": "Timestamp of the activity",
                                                                        "type": "string",
                                                                        "format": "date-time"
                                                                    }
                                                                },
                                                                "type": "object"
                                                            }
                                                        }
                                                    },
                                                    "type": "object"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
